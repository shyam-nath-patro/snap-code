/* find the distance of two characters
char1 is comes before char2 
using function method*/
#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int diff(char a, char b) {
        return b-a;
}

int main() {
    char ch1, ch2;
    scanf(" %c %c", &ch1, &ch2);
    int distance = diff(ch1, ch2);
    printf("The distance between %c and %c is %d", ch1,ch2,distance);
    return 0;
}
